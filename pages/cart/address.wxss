/* pages/cart/address.wxss */
@import '../../pages/templates/dataEmptyTemplate.wxss';
@import '../../pages/home.wxss';

.takeSelfList {
  display: flex;
  flex-direction: column;
  justify-content: center;
  -webkit-display: flex;
  -webkit-flex-direction: column;
  -webkit-justify-content: center;
}

.takeSelfListItem {
  display: flex;
  flex-direction: column;
  justify-content: left;
  -webkit-display: flex;
  -webkit-flex-direction: column;
  -webkit-justify-content: left;
  width: 100%;
  font-size: 28rpx;
  text-align: left;
  align-items: center;
  color: #696969;
  /* margin: 20rpx; */
  margin-left: 10rpx;
  margin-right: 10rpx;
  margin-bottom: 20rpx;
}

.takeSelfListItem text {
  width: 100%;
  margin-bottom: 20rpx;
}

.line {
  background: #e0e3da;
  width: 100%;
  height: 2rpx;
}